#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/vinxi@0.5.10_db0@0.3.4_ioredis@5.9.0_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vinxi/bin/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/vinxi@0.5.10_db0@0.3.4_ioredis@5.9.0_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vinxi/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/vinxi@0.5.10_db0@0.3.4_ioredis@5.9.0_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/vinxi@0.5.10_db0@0.3.4_ioredis@5.9.0_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vinxi/bin/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/vinxi@0.5.10_db0@0.3.4_ioredis@5.9.0_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules/vinxi/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/vinxi@0.5.10_db0@0.3.4_ioredis@5.9.0_jiti@2.6.1_lightningcss@1.30.2_terser@5.44.1/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vinxi/bin/cli.mjs" "$@"
else
  exec node  "$basedir/../vinxi/bin/cli.mjs" "$@"
fi
