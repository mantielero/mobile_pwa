#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules/@vinxi/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/@vinxi+listhen@1.5.6/node_modules:/home/jose/Documentos/src/web/mobile_pwa/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/listhen.mjs" "$@"
else
  exec node  "$basedir/../../../../../@vinxi+listhen@1.5.6/node_modules/@vinxi/listhen/bin/listhen.mjs" "$@"
fi
